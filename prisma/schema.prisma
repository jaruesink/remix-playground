// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Test {
  id    Int      @id @default(autoincrement())
  hello String       @default("world")
}

model Theme {
  id        Int      @id @default(autoincrement())
  name      String
  color_50  String
  color_100 String
  color_200 String
  color_300 String
  color_400 String
  color_500 String
  color_600 String
  color_700 String
  color_800 String
  color_900 String
}